<odoo>
    <data>

        <record id="product_template_only_form_view_inherit_informes_presupuestos" model="ir.ui.view">
            <field name="name">product.template.only.form.view.inherit.informes.presupuestos</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='barcode']" position="before">
                    <field name="codigo_fabricante" readonly="1" />
                    <field name="division" readonly="1" />
                </xpath>

            </field>
        </record>


    </data>
</odoo>