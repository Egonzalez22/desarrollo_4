<odoo>
    <data>

        <template id="report_dymo_sinprecio">
            <t t-call="web.basic_layout">
                <div class="page">
                    <t t-call="etiqueta_analitica.report_productlabel_dymo_sinprecio">
                        <t t-set="products" t-value="products" />
                    </t>
                </div>
            </t>
        </template>

        <template id="report_productlabel_dymo_sinprecio">
            <t t-call="web.html_container">
                <t t-set="barcode_size" t-value="'width:45.5mm; height:7.5mm'" />
                <t t-set="table_style" t-value="'width:100%; height:32mm;'" />
                <t t-set="padding_page" t-value="'padding: 2mm'" />

                <t t-foreach="quantity.items()" t-as="barcode_and_qty_by_product">
                    <t t-set="product" t-value="barcode_and_qty_by_product[0]" />
                    <t t-foreach="barcode_and_qty_by_product[1]" t-as="barcode_and_qty">
                        <t t-set="barcode" t-value="barcode_and_qty[0]" />
                        <t t-foreach="range(barcode_and_qty[1])" t-as="qty">
                            <t t-call="etiqueta_analitica.report_simple_label_dymo_sinprecio" />
                        </t>
                    </t>
                </t>
            </t>
        </template>

        <template id="report_simple_label_dymo_sinprecio">
            <div class="o_label_sheet o_label_dymo" t-att-style="padding_page">
                <div class="o_label_full" t-att-style="table_style">
                    
                    <div class="text-start o_label_small_barcode" style="background-color: blue;">
                        <t t-if="barcode">
                            <!-- `quiet=0` to remove the left and right margins on the barcode -->
                            <div t-out="barcode" style="padding:0" t-options="{'widget': 'barcode', 'quiet': 0, 'symbology': 'auto', 'img_style': barcode_size}" />
                            <div class="o_label_name" style="height:1.7em; background-color: transparent;">
                                <span t-out="barcode" />
                            </div>
                        </t>
                    </div>

                    <div style="background-color: purple; font-size: 7px !important;line-height: 2px;">
                        <div style="background-color: orange; float:left; width: 75%">
                            <div style="background-color: pink;"><strong>Código:</strong> </div>
                            <div style="background-color: pink;"><strong>Cliente:</strong> <span t-out="picking.partner_id.name"></span></div>
                            <div style="background-color: pink;"><strong>Fecha:</strong> <span t-out="picking.date_done"></span></div>
                            <div style="background-color: pink;"><strong>Lote:</strong> </div>
                            <div style="background-color: pink;"><strong>Solicitud:</strong> <span t-out="picking.name"></span></div>
                        </div>
                        <div style="background-color: green; float:left; width: 25%">
                            <!-- logo de la compa;ia con t-esc -->  
                            <div style="width: 20px; height: 30px; background: red;">
                                <!-- <img t-att-src='image_data_uri(o.company_id.logo)' style="width: 100%" /> -->
                            </div>
                        </div>
                        <div style="clear:both"></div>
                    </div>
                    
                   

                    <!-- <div class="o_label_name" style="line-height: 100%;background-color: transparent;padding-top: 1px;">
                        <span t-if="product.is_product_variant" t-field="product.display_name" />
                        <span t-else="" t-field="product.name" />
                    </div> -->
                    
                    <!-- <div class="o_label_left_column" style="background-color: red;">
                        <div style="font-size:9px">Código: <span t-field="product.name" /></div>
                        <div style="font-size:9px">Cliente: <span t-field="product.name" /></div>
                        <div style="font-size:9px">Fecha: <span t-field="product.name" /></div>
                        <div style="font-size:9px">Lote: <span t-field="product.name" /></div>
                        <div style="font-size:9px">Nro. Solicitud: <span t-field="product.name" /></div>
                    </div> -->

                    <!-- <div class="text-end" style="padding: 0 4px; background-color: purple;">
                        footer
                        <t t-if="product.is_product_variant">
                            <strong class="o_label_price_small" t-field="product.lst_price" t-options="{'widget': 'monetary', 'label_price': True}" />
                        </t>
                        <t t-else="">
                            <strong class="o_label_price_small" t-field="product.list_price" t-options="{'widget': 'monetary', 'label_price': True}" />
                        </t>
                        <div t-if="False" class="o_label_extra_data">
                            <t t-out="extra_html" />
                        </div>
                    </div> -->
                </div>
            </div>
        </template>

        <record id="report_product_template_label_dymo_sinprecio" model="ir.actions.report">
            <field name="name">Product Label Without Price (PDF)</field>
            <field name="model">product.template</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">etiqueta_analitica.report_dymo_sinprecio</field>
            <field name="report_file">etiqueta_analitica.report_dymo_sinprecio</field>
            <field name="paperformat_id" ref="product.paperformat_label_sheet_dymo" />
            <field name="print_report_name">'Etiqueta - %s' % (object.name)</field>
            <field name="binding_type">report</field>
        </record>

    </data>
</odoo>