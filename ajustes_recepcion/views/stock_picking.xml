<odoo>
    <data>
            <record id="view_picking_form" model="ir.ui.view">
                <field name="name">view.picking.form.inherit</field>
                <field name="model">stock.picking</field>
                <field name="inherit_id" ref="stock.view_picking_form"/>
                <field name="arch" type="xml">

                <xpath expr="//field[@name='product_uom_qty']" position="replace">
                    <field name="product_uom_qty" string="Cant. Solicitada"/>
                </xpath>

                <xpath expr="//field[@name='quantity_done']" position="replace">
                    <field name="quantity_done" string="Cant. Recibidas"/>
                </xpath>

            </field>
        </record>

        <record id="view_stock_move_line_operation_tree_inherit" model="ir.ui.view">
            <field name="name">view.stock.move.line.operation.tree.inherit</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='cantidad_impresiones']" position="replace">
                    <field name="cantidad_impresiones" string="Cant. de Etiquetas"/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>