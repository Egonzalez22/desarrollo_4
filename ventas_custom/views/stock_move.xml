<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_stock_move_operations_inherit_ventas_custom" model="ir.ui.view">
            <field name="name">view.stock.move.operations.inherit.ventas.custom</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_stock_move_operations" />
            <field name="arch" type="xml">

                <xpath expr="//footer" position="inside">
                    <button name="action_generar_nro_serie" string="Generar Series" type="object" class="btn btn-warning" />
                </xpath>

            </field>
        </record>


        <record id="view_stock_move_line_operation_tree_inherit_ventas_custom" model="ir.ui.view">
            <field name="name">view.stock.move.line.operation.tree.inherit.ventas.custom</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree" />
            <field name="arch" type="xml">

                <xpath expr="//tree" position="inside">
                    <field name="codigo_muestra" />
                    <field name="nro_lote" />
                    <field name="fecha_vencimiento_lote" string="Fecha Vencimiento" />
                    <field name="observaciones"/>
                    <field name="cantidad_impresiones" string="Cant. ImpresiÃ³n" />
                </xpath>

            </field>
        </record>

    </data>
</odoo>