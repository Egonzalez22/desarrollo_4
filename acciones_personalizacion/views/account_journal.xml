<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="account_journal_personalizacion" model="ir.ui.view">
            <field name="name">account.journal.personalizacion</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='default_account_id']" position="after">
                    <field name="transferencia_automatica" />
                </xpath>
                <xpath expr="//field[@name='currency_id']" position="before">
                    <field name="cuenta_transferencia_automatica" />
                     <field name="control_bloqueo" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>