<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="hr_payslip_standard_run_form_txt" model="ir.ui.view">
            <field name="name">hr.payslip.standard.run.form.txt</field>
            <field name="model">hr.payslip.run</field>
            <field name="inherit_id" ref="hr_payroll.hr_payslip_run_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet" position="inside">
                    <group>
                        <group>
                            <field name="referencia_generada" readonly="1"/>
                            <field name="importe_generado" readonly="1"/>
                            <field name="cant_documentos_generados" readonly="1"/>
                        </group>
                        <group>
                        </group>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>