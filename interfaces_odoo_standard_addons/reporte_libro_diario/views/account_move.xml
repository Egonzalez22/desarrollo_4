<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="account_move_form_inherit" model="ir.ui.view">
            <field name="name">account_move_form_inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@id='other_tab_group']/group/field[@name='company_id']/.." position="inside">
                    <field name="secuencia_asiento"/>
                </xpath>
                <xpath expr="//group[@id='other_tab_entry_group']/group/field[@name='company_id']/.." position="inside">
                    <field name="secuencia_asiento"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>