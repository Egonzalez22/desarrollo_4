        <!-- Balance general -->
        <record id="account_financial_report_bg_l10n_py" model="account.report">
            <field name="name">Balance General</field>
            <field name="country_id" ref="base.py"/>
            <field name="filter_hierarchy">never</field>
            <field name="filter_unfold_all" eval="True"/>
            <field name="filter_partner" eval="True"/>
            <field name="filter_analytic" eval="True"/>
            <field name="filter_analytic_groupby" eval="True"/>
            <field name="filter_multi_company">selector</field>
            <field name="column_ids">
                <record id="account_financial_report_l10n_py_bg_column" model="account.report.column">
                    <field name="name">Saldo</field>
                    <field name="expression_label">balance</field>
                </record>
            </field>
            <field name="line_ids">
                <record id="account_financial_report_line_py_A_ROOT" model="account.report.line">
                    <field name="name">Activo</field>
                    <field name="code">AC_ROOT</field>
                    <field name="aggregation_formula">AC_GRP.balance+ANC_GRP.balance</field>
                    <field name="foldable" eval="True"/>
                    <field name="hide_if_zero" eval="True"/>
                    <field name="children_ids">
                        <record id="account_financial_report_line_py_AC_GRP" model="account.report.line">
                            <field name="name">Activo corriente</field>
                            <field name="code">AC_GRP</field>
                            <field name="foldable" eval="True"/>
                            <field name="hide_if_zero" eval="True"/>
                            <field name="aggregation_formula">DIS_LINE.balance+INV_TEMP_LINE.balance+CRED_LINE.balance+INV_LINE.balance+GTO_PAG_AD_LINE.balance+OT_AC_LINE.balance</field>
                            <field name="children_ids">
                                <record id="account_financial_report_line_DIS_line" model="account.report.line">
                                    <field name="name">Disponibilidades</field>
                                    <field name="code">DIS_LINE</field>
                                    <field name="foldable" eval="True"/>
                                    <field name="hide_if_zero" eval="True"/>
                                    <field name="groupby">account_id</field>
                                    <field name="expression_ids">
                                        <record id="account_financial_report_line_DIS_line_expr" model="account.report.expression">
                                            <field name="label">balance</field>
                                            <field name="engine">domain</field>
                                            <field name="date_scope">from_beginning</field>
                                            <field name="formula">[('account_id.tag_ids.name', '=', 'Disponibilidades')]</field>
                                            <field name="subformula">sum</field>
                                        </record>
                                    </field>
                                </record>
                                <record id="account_financial_report_line_INV_TEMP_line" model="account.report.line">
