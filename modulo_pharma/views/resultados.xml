<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- ALTA COMPLEJIDAD -->
        <record id="res_alta_complejidad_tree_view" model="ir.ui.view">
            <field name="name">alta_complejidad.tree.view</field>
            <field name="model">resultado.alta</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="workorder_id" invisible="1"/>
                    <field name="sustancia_activa_id"/>
                    <field name="resultado"/>
                    <field name="resultado_txt"/>
                    <field name="lod"/>
                    <field name="intervalo_referencia"/>
                </tree>
            </field>
        </record>

        <record id="res_alta_complejidad_form_view" model="ir.ui.view">
            <field name="name">alta_complejidad.form.view</field>
            <field name="model">resultado.alta</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="workorder_id" invisible="1"/>
                            <field name="sustancia_activa_id"/>
                            <field name="resultado"/>
                            <field name="resultado_txt"/>
                            <field name="lod"/>
                            <field name="intervalo_referencia"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="res_alta_complejidad_form_tablet" model="ir.ui.view">
            <field name="name">res.alta.complejidad.form.tablet</field>
            <field name="model">resultado.alta</field>
            <field name="priority">200</field>
            <field name="arch" type="xml">
                <form class="o_workorder_tablet_form">
                    <field name="workorder_id" invisible="1"/>
                    <div class="o_workorder_bar">
                        <div class="o_workorder_bar_content">
                            <field name="sustancia_activa_id" />
                            <field name="resultado" />
                            <field name="resultado_txt"/>
                            <field name="lod" class="col-md-2" />
                            <field name="intervalo_referencia"/>
                        </div>
                    </div>
                </form>
            </field>
        </record>

