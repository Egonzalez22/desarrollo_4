<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="recibo_pagos_inherit_personalizacion" model="ir.ui.view">
            <field name="name">recibo.pagos.inherit.personalizacion</field>
            <field name="model">account.payment</field>
            <field name="inherit_id" ref="account.view_account_payment_form" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='is_internal_transfer']" position="before">
                    <!-- <field name="id" readonly="1" string="Numero Recibo Interno" /> -->
                    <field name="numero_recibo_original" readonly="1"  invisible="1" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>