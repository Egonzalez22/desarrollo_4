<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="quality_point_inherit_personalizacion" model="ir.ui.view">
            <field name="name">quality.point.inherit.personalizacion</field>
            <field name="model">quality.point</field>
            <field name="inherit_id" ref="quality.quality_point_view_form" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='picking_type_ids']" position="after">
                    <field name="fecha_inicio" attrs="{'invisible': [('company_id', '!=', 2)]}" />
                    <field name="fecha_finalizacion"  attrs="{'invisible': [('company_id', '!=', 2)]}"/>

                </xpath>
            </field>
        </record>
    </data>
</odoo>